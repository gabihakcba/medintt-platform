generator client {
  provider        = "prisma-client-js"
  output          = "../src/generated/client"
  previewFeatures = ["views"]
  binaryTargets   = ["native", "debian-openssl-3.0.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL_MEDINTT4")
}

model Abonos {
  Id                     Int            @id(map: "PK_Abonos") @default(autoincrement())
  Id_Prestataria         Int?
  FechaAlta              DateTime?      @db.Date
  Fecha_Inicio_Abono     DateTime?      @db.Date
  ImporteConceptoMensual Decimal?       @db.Decimal(18, 6)
  ConceptoMensual        String?        @db.NVarChar(Max)
  CantidadMeses          Int?
  Activo                 Int?
  Prestatarias           Prestatarias?  @relation(fields: [Id_Prestataria], references: [Id], onDelete: Cascade, map: "FK_Abonos_Prestatarias")
  AbonosxMeses           AbonosxMeses[]
}

model AbonosxMeses {
  Id                     Int                      @id(map: "PK_AbonosxMeses")
  Id_Abono               Int?
  Fecha                  DateTime?                @db.Date
  IndiceInflacion        Decimal?                 @db.Decimal(18, 6)
  Id_Factura             Int?
  Total_Mensual          Decimal?                 @db.Decimal(18, 6)
  Abonos                 Abonos?                  @relation(fields: [Id_Abono], references: [Id], onDelete: Cascade, map: "FK_AbonosxMeses_Abonos")
  AbonosxMeses_Conceptos AbonosxMeses_Conceptos[]
}

model AbonosxMeses_Conceptos {
  Id           Int           @id(map: "PK_AbonosxMeses_Conceptos") @default(autoincrement())
  Id_AbonoxMes Int?
  Concepto     String?       @db.NVarChar(Max)
  Importe      Decimal?      @db.Decimal(18, 6)
  AbonosxMeses AbonosxMeses? @relation(fields: [Id_AbonoxMes], references: [Id], onDelete: Cascade, map: "FK_AbonosxMeses_Conceptos_AbonosxMeses")
}

model Afiliacion_Pacientes {
  Id                   Int                   @id(map: "PK_Afiliacion_Pacientes") @default(autoincrement())
