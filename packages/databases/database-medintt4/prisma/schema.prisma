  Fecha_Hasta              DateTime?                  @db.SmallDateTime
  Fecha_Reincoporacion     DateTime?                  @db.SmallDateTime
  Activo                   Int?
  Status                   String?                    @db.NVarChar(50)
  Diagnostico              String?                    @db.NVarChar(Max)
  Evolucion                String?                    @db.NVarChar(Max)
